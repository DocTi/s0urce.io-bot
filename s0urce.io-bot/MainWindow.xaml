<Window x:Class="s0urce.io_bot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:s0urce.io_bot"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="ProcessListBox" HorizontalAlignment="Right" VerticalAlignment="Top" Width="600" Height="30" Margin="0,10,10,0" VerticalContentAlignment="Center"/>
        <Button Content="Set Cdm window" HorizontalAlignment="Right" Margin="0,50,10,0" VerticalAlignment="Top" Width="100" Height="35" Click="Button_Click"/>
        <Button x:Name="TestButton" Content="Test" HorizontalAlignment="Left" Margin="310,172,0,0" VerticalAlignment="Top" Width="100" Height="30" Click="TestButton_Click"/>
        <TextBlock x:Name="TestLog" HorizontalAlignment="Left" Margin="10,92,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="272" Width="265"/>

    </Grid>
</Window>
